ninja_required_version = 1.5.1 
bs_package_flags = -bs-package-name bucklescript_app
src_root_dir = /Users/jackalcooper/elixir/missing_pieces/assets
bsc = /Users/jackalcooper/elixir/missing_pieces/assets/node_modules/bs-platform/bin/bsc.exe
bsdep = /Users/jackalcooper/elixir/missing_pieces/assets/node_modules/bs-platform/bin/bsb_helper.exe
ocamllex = ocamllex.opt
bsc_flags = -nostdlib -I '/Users/jackalcooper/elixir/missing_pieces/assets/node_modules/bs-platform/lib/ocaml' -no-alias-deps -color always -w -40+6+7+27+32..39+44+45
ppx_flags = 
bs_package_includes = -I /Users/jackalcooper/elixir/missing_pieces/assets/node_modules/bucklescript-tea/lib/ocaml
bs_package_dev_includes = 
refmt = /Users/jackalcooper/elixir/missing_pieces/assets/node_modules/bs-platform/bin/refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsb_dir_group = 0
bsc_lib_includes = -I bs
rule build_ast_and_deps
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build bs/main.mlast : build_ast_and_deps $src_root_dir/bs/main.ml
rule build_deps
  command = ${bsdep} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build bs/main.mlast.d : build_deps bs/main.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${bsc_flags} -o ${in} -c  ${in} ${postbuild}
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build bs/main.cmj $src_root_dir/lib/js/bs/main.js bs/main.cmi : build_cmj_cmi bs/main.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:lib/js/bs
build build.ninja : phony  ||  bs/main.mlast.d
